#include <iostream>
using namespace std;

float calcularMediaVetor(int* v, int tamanho) {
    int soma = 0;
    for (int i = 0; i < tamanho; i++) {
        soma += v[i];
    }
    return soma / (float)tamanho;
}

int main() {
    int vetor[] = {2, 4, 6, 8, 10};
    int tamanho = sizeof(vetor) / sizeof(vetor[0]);

    cout << "MÃ©dia: " << calcularMediaVetor(vetor, tamanho) << endl;

    return 0;
}
