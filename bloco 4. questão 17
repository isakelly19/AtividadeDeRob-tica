#include <iostream>
using namespace std;

void alterarValores(int valor, int &ref) {
    valor = 10;
    ref = 20;
}

int main() {
    int a = 1, b = 2;

    cout << "Antes: a = " << a << ", b = " << b << endl;
    alterarValores(a, b);
    cout << "Depois: a = " << a << ", b = " << b << endl;

    return 0;
}
