#include <iostream>
using namespace std;

void aprovado(float* nota) {
    if (*nota >= 7)
        cout << "Aprovado" << endl;
    else
        cout << "Reprovado" << endl;
}

int main() {
    float nota;

    cout << "Digite a nota do aluno: ";
    cin >> nota;

    aprovado(&nota);

    return 0;
}
